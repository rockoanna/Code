#include <iostream>

using namespace std;

int main()
{
    int n;
    cin >> n;

    while(n != 0){
        int case_nr = 1;
        int count_0 = 0;
        int crt;
        for(int i = 0; i < n; i++){
            cin >> crt;
            if(crt == 0){
                count_0 +=1;
            }
        }
        cout << "Case " n - 2*count_0 << endl;
        cin >> n;
        case_nr+= 1
    }


    return 0;
}
